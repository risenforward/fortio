connect:
  host: 127.0.0.1
  port: 5672

exchange:
  trade_after_strike:
    name: peatio.trade.after_strike
    type: fanout

queue:
  matching:
    name: peatio.matching
  new_trade:
    name: peatio.trade.new
  pusher:
    name: peatio.pusher
  withdraw_audit:
    name: peatio.withdraw.audit
  withdraw_coin:
    name: peatio.withdraw.coin
  mailer:
    name: peatio.mailer

binding:
  matching:
    queue: matching
  trade_executor:
    queue: new_trade
  pusher:
    queue: pusher
    exchange: trade_after_strike
  withdraw_audit:
    queue: withdraw_audit
  withdraw_coin:
    queue: withdraw_coin
  mailer:
    queue: mailer
