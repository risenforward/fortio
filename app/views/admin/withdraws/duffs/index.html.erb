<% unless @one_duffs.empty? %>
  <div class="panel panel-primary">
    <div class="panel-heading">
      <span><%= t('.one') %></span>
    </div>
    <%= render partial: 'table', locals: {duffs: @one_duffs} %>
  </div>
<% end %>
<% unless @all_duffs.empty? %>
  <div class="panel panel-success">
    <div class="panel-heading">
      <span><%= t('.all') %></span>
    </div>
    <%= render partial: 'table', locals: {duffs: @all_duffs} %>
  </div>
<% end %>

<% if @all_duffs.empty? and @one_duffs.empty? %>
  <h3 class="text-info text-center"><%= t('.empty') %></h3>
<% end %>
