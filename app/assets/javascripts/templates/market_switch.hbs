{{#if current}}
<tr class='highlight' data-market='{{market}}'>
{{else}}
<tr data-market='{{market}}'>
{{/if}}
  <td class='market col-xs-6'>{{format_market data.base_unit data.quote_unit}}</td>
  <td class='price bid col-xs-9 text-right {{format_trend data.buy_trend}}'>{{data.buy}} {{{trend_icon data.buy_trend}}}</td>
  <td class='price ask col-xs-9 text-right {{format_trend data.sell_trend}}'>{{data.sell}} {{{trend_icon data.sell_trend}}}</td>
</tr>
